{
    "name": "ddedic/veemo-core",
    "description": "Veemo Core package",
    "keywords": [
        "veemo", "core"
    ],
    "homepage": "https://github.com/ddedic/veemo-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Danijel Dedic",
            "email": "dedic.d@gmail.com",
            "homepage": "http://veemo.ddedic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/view": "~5.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/config": "~5.0",
        "illuminate/routing": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Veemo\\Core\\": "src"
        },
        "classmap": [
            "src/Themes/Exceptions/Exceptions.php"
        ],

        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Veemo\\Core\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
